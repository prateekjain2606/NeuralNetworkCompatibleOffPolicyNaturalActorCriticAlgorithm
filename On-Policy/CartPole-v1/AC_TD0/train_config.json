{
    "env": "CartPole-v1",
    "gamma": 0.99,
    "iterations": 1000,
    "episode_length": null,
    "numpy_seed": 1,
    "environment_seed": 1,
    "pytorch_seed": 1,
    "render": false,
    "rewards_path": "cartpole_rewards.json",
    "record": false,
    "recording_path": "cartpole_recording/",
    "recording_frequency": 10,
    "actor": {
        "hidden_layer_neurons": [
            16
        ],
        "learning_rate": 0.001,
        "load": false,
        "load_path": "model/actor_final.pkl",
        "initial_save_path": "model/actor_initial.pkl",
        "final_save_path": "model/actor_final.pkl"
    },
    "critic": {
        "hidden_layer_neurons": [
            64,
            64
        ],
        "learning_rate": 0.005,
        "load": false,
        "load_path": "model/critic_final.pkl",
        "initial_save_path": "model/critic_initial.pkl",
        "final_save_path": "model/critic_final.pkl"
    },
    "learning_rate_scheduler": {
        "required": true,
        "schedule": [
            [
                [
                    400,
                    500
                ],
                {
                    "lr_A": 0.000001,
                    "lr_C": 0.000001
                }
            ]
        ]
    }
}